/************************************ 机器人弹窗菜单 ****************************************/
.moveMenu{
	position: absolute;
	top:50%;
	left:50%;
	width:200px;
	height:200px;
	margin-top: -100px;
	margin-left: -100px;
	/*border:1px solid #0366F9;*/
	transform-style:preserve-3d;
	transform:perspective(800px) rotateX(-10deg) rotateY(0deg);
	-webkit-border-radius: 100%;
	-moz-border-radius: 100%;
	-o-border-radius:100%;
	-ms-border-radius: 100%;
	border-radius: 100%;
	display:none;
	background:#eff1f3;
	-moz-box-shadow: 0px 2px 16px 5px #1C5081;
	-webkit-box-shadow: 0px 2px 16px 5px #1C5081;
	box-shadow: 0px 2px 16px 5px #1C5081;
}
.moveMenu ul{
	position: absolute;
	top:50%;
	left:50%;
	width:0;
	height:0;
}
.moveMenu ul li{
    position: absolute;
    top: -30px;
    left: -30px;
    width: 60px;
    height: 60px;
    text-align: center;
    border: 1px solid #05A5EE;
    -webkit-border-radius: 100%;
    -moz-border-radius: 100%;
    -o-border-radius: 100%;
    -ms-border-radius: 100%;
    border-radius: 100%;
    transition: all 1s;
    cursor: pointer;
    padding: 16px;
}
.moveMenu ul li:hover{
	background: #449CF9;
	color: #FFFFFF;
}
.moveMenu ul li.menu1{
	top: -93px;
}
.moveMenu ul li.menu2{
    top: -30px;
    left: -92px;
}
.moveMenu ul li.menu3{
    top: 33px;
    left: -30px;
}
.moveMenu ul li.menu4{
	top: -30px;
    left: 32px;
}
.closeMenu{
	position: absolute;
	top:50%;
	left:50%;
	width: 50px;
	height:50px;
	margin-top: -25px;
	margin-left: -25px;
	-webkit-border-radius:100%;
	-moz-border-radius:100%;
	-o-border-radius:100%;
	-ms-border-radius:100%;
	border-radius:100%;
	background: url("../images/closeMenu.png") no-repeat center center;
	background-size: cover;
	cursor:pointer;
}
.closeMenu:hover{
	border:1px dashed #01599C;
}
.moveMenuContent{
	position: absolute;
	top: 50%;
	left: 50%;
	width:500px;
	height:300px;
	margin-left: -250px;
	margin-top: -150px;
	border: 1px solid #0452A4;
	background: #8F9497;
	-moz-box-shadow: 0px 2px 16px 5px #1C5081;
	-webkit-box-shadow: 0px 2px 16px 5px #1C5081;
	box-shadow: 0px 2px 16px 5px #1C5081;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	-o-border-radius:5px;
	-ms-border-radius: 5px;
	border-radius: 5px;
	display: none;
}
.moveMenuContent .closeMenuContent{
	position: absolute;
    top: 3px;
    right: 8px;
    width: 20px;
    height: 20px;
    color: #082db9;
    transition: all 0.4s ease-in-out;
    -webkit-transition: all 0.4s ease-in-out;
    -moz-transition: all 0.4s ease-in-out;
    -o-transition: all 0.4s ease-in-out;
    cursor: pointer;
}
.moveMenuContent .closeMenuContent:hover{
	transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
    -moz-transform: rotate(360deg);
    -o-transform: rotate(360deg);
    -ms-transform: rotate(360deg);
   -webkit-transform-origin: center center;
	transform-origin: center center;
}
