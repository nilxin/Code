					mouseenter:function(){
						$(".month-list").show(300);
					},
					mouseleave:function(){
						$(".month-list").hide(300);
					},
					click:function(){
						var choose=$(".month-list");
						if(choose.css("display")=="block"){
							choose.hide(300);
						}else{
							choose.show(300);
						}
						
					}
				})

				$(".month-list li").on({
					click:function(){
						var val=$(this).html();
						$(".choose").html(val);
						$(".month-list").hide(300);
					}
