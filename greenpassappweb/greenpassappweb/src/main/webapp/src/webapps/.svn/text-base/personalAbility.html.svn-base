<div id="InspectionReview">

	<style type="text/css">
		.header{margin-bottom: 25px;height: 40px;line-height: 40px;color: #fff;}
		.btn{height:36px;line-height:17px;font-size:17px;margin-left: 15px;box-shadow: 0 0 5px 1px rgba(0,0,0,0.5);
    		color: #3a3a3a;}
		
		.month-list{
			background: #1c92ff;
			position: relative;
			margin-top: 9px;
			width: 94px;
			display: none;
		}
		.month-list li{
			padding-left: 5px;
			font-size: 14px;
		}
		.month-list li:hover{
			background: #006ab9;
		}
		.choose{
		    background: #1c92ff;
		    font-size: 16px;
		    padding: 0px 15px;
		    border-radius: 10px;
		    cursor: pointer;
		    width: 94px;
		}
		.month-list .month-list_first:before{
			content: '';
		    width: 0;
		    height: 0;
		    position: absolute;
		    right: 0.75em;
		    margin-right: -4px;
		    top: -9px;
		    border-style: solid;
		    border-width: 0 9px 9px 9px;
		    border-color: transparent transparent #1c92ff transparent;
		}
		.wrapper { display: grid; 
		grid-template-columns: 150px 10px 600px 10px 600px 10px 600px; 
		grid-template-rows: 400px 10px 400px 10px 400px 10px 400px 10px 400px; } 
		

		.a { grid-area: 1 / 1 / 2 / 2; } 
		.b { grid-area: 1 / 3 / 2 / 4; } 
		.c { grid-area: 1 / 5 / 2 / 6; } 
		.d { grid-area: 1 / 7 / 2 / 8; } 

		.e { grid-area: 3 / 1 / 4 / 2; } 
		.f { grid-area: 3 / 3 / 4 / 4; } 
		.g { grid-area: 3 / 5 / 4 / 6; } 
		.h { grid-area: 3 / 7 / 4 / 8; } 

		.i { grid-area: 5 / 1 / 6 / 2; } 
		.j { grid-area: 5 / 3 / 6 / 4; }
		.k { grid-area: 5 / 5 / 6 / 6; }
		.l { grid-area: 5 / 7 / 6 / 8; }

		.m { grid-area: 7 / 1 / 8 / 2; }
		.n { grid-area: 7 / 3 / 8 / 4; }
		.o { grid-area: 7 / 5 / 8 / 6; }
		.p { grid-area: 7 / 7 / 8 / 8; }

		.q { grid-area: 9 / 1 / 10 / 2; }
		.r { grid-area: 9 / 3 / 10 / 4; }
		.s { grid-area: 9 / 5 / 10 / 6; }
		.t { grid-area: 9 / 7 / 10 / 8; }
		
		.u { grid-area: 11 / 1 / 12 / 2; }
		.v { grid-area: 11 / 3 / 12 / 4; }
		.w { grid-area: 11 / 5 / 12 / 6; }
		.x { grid-area: 11 / 7 / 12 / 8; }

	</style>

