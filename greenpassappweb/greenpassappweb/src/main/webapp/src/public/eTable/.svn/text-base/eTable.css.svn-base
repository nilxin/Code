/*.eTable-pager .end-page.disabled{background-position:right -216px !important;}*/
.eTable-pager .last-page:after,
.eTable-pager .next-page:after{
    content: '';
    /*background: url(images/image_arrow_right_black.png) no-repeat;*/
    width: 20px;
    height: 20px;
    position: absolute;
    top: 5px;
    left: 5px;
}
/*eTable loading*/
.eTable-loading{
	width:100%;
	height:100%;
}
.eTable-loading div{
	position:absolute;
	z-index:999;
	width:100%;
	height:100%;
	background-color:#ccc;
	opacity:0.5;
	filter:alpha(opacity=50);
	top:0;left:0;
}
.eTable-loading span{
	display:block;
	width:200px;
	height:50px;
	line-height:50px;
	border:solid 1px #ccc;
	text-align:center;
	position:absolute;
	background-color:#fff;
	top:50%;
	left:50%;
	margin:-25px 0 0 -100px;
	opacity:1;
	z-index:9999;
	border-radius:6px;
}
.eTable-indent{
	display:inline-block;
	width:12px;
}

.eTable-tree-collapsed, .eTable-tree-expanded{
	display:inline-block;
	width:6px;
	height:9px;
	background:url(images/tree_icons.png) no-repeat;
	margin:0 6px 0 0;
	cursor:pointer;
}
.eTable-tree-collapsed{background-position:-6px -6px;}
.eTable-tree-expanded{background-position:-22px -6px;}

.eTable-button-reset,.eTable-button-view,.eTable-button-edit,.eTable-button-run,.eTable-button-stop,.eTable-button-logs,.eTable-button-data,.eTable-button-delete{
	background-repeat:no-repeat;
	background-position:center;
	border:none!important;
	padding:0 0 0 20px!important;
	margin:0 3px!important;
	height:20px!important;
	cursor:pointer;
	/*float:left;*/
	display: inline-block;
    line-height: 20px;
}
.eTable-button-view:hover,
.eTable-button-reset:hover,
.eTable-button-edit:hover,
.eTable-button-run:hover,
.eTable-button-stop:hover,
.eTable-button-logs:hover,
.eTable-button-data:hover,
.eTable-button-delete:hover{
	color:#319fed;
}
.eTable-button-edit{
	background:url(images/btn-edit.png) left center no-repeat;
}
.eTable-button-run{
	background:url(images/btn-run.png) left center no-repeat;
}
.eTable-button-stop{
	background:url(images/btn-stop.png) left center no-repeat;
}
.eTable-button-logs{
	background:url(images/btn-logs.png) left center no-repeat;
}
.eTable-button-data{
	background:url(images/btn-data.png) left center no-repeat;
}
.eTable-button-delete{
	background:url(images/btn-delete1.png) left center no-repeat;
}
.eTable-button-view{
	background:url(images/btn-view.png) left center no-repeat;
}
.eTable-button-reset{
	background:url(images/btn-reset.png) left center no-repeat;
}
.disabledBtn{
    color: #c9c9c9 !important;
}
/*sort button*/
.eTable-sort-up, .eTable-sort-down{background:url(images/eTable-button-icon.png) 0 -200px no-repeat;width:10px;height:12px;display:inline-block;border:none;outline:none;cursor:pointer;margin:0 5px;overflow:hidden;text-indent:-100px;margin-top:3px;}
.eTable-sort-up{background-position:0 -222px;}

/*fixed height*/
.fixedHeight .eTable-header th {
	height: 41px;
}

.fixedHeight .eTable-header div.v {
	height: 24px!important;
	line-height: 24px!important;
}

.fixedHeight .eTable-header div.v .checkbox {
	margin-top: 5px;
}

.fixedHeight .eTable-list div.v {
	height: 22px!important;
	line-height: 22px!important;
	color: #505050;
}

.fixedHeight div.v {
	padding-top: 0!important;
	padding-bottom: 0!important;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	cursor: default;
}
.changePageInput {
    width: 40px;
    padding: 0;
    border: solid 1px #c7c7c7;
    height: 25px;
    color: #6e808e;
    border-radius: 3px;
    line-height: 25px;
    margin: 0 4px;
    text-align: center;
}
.eTable-pager a.changePageBtn {
    border-radius: 5px;
    padding: 0;
    height: 22px;
    width: 50px;
    display: inline-block;
    line-height: 18px;
    background: #3c87c6;
    border: 1px #3c87c6 solid;
    color: #fff;
}
.eTable-pager .GoToPage{
    display: inline-block;
    line-height: 30px;
    height: 30px;
    position: relative;
    /*top: -1px;
    margin-left: 20px;*/
}
.eTable-pager .currentShow {
    color: #f24242;
    font-size: 16px;
}
.eTable-pager .data-total {
    margin-right: 5px;
}

/*edit*/
.eTable-list .edit div.v{height:auto!important;line-height:auto!important;overflow:hidden;}
.eTable-list .edit div.v input{border:solid 1px #ccc;background:#eee;width:100%;height:16px;line-height:16px;padding:3px 0;padding:0\9;}
.eTable-list .edit div.v input.editing{border-color:#999;background-color:#fff;}
.fixedHeight .eTable-list div.v i{font-style:normal;display:inline-block;width:auto;height:22px;text-align:center;color:#fff;border-radius:10px;padding:0 10px;}
.fixedHeight .eTable-list div.v i.bg_red{background:#ff0500;}
.fixedHeight .eTable-list div.v i.bg_yellow{background:#FFD200;}
.fixedHeight .eTable-list div.v i.bg_green{background:#66bb6a;}
.fixedHeight .eTable-list div.v i.bg_grey{background:#999999;}
.fixedHeight .eTable-list div.v i.color_red{color:#ff0500;}
.fixedHeight .eTable-list div.v i.color_yellow{color: #FFD200;}
.fixedHeight .eTable-list div.v i.color_green{color: #66bb6a;}
.fixedHeight .eTable-list div.v i.color_grey{color:#999999;}

.Top-Pager{
    position: absolute;
    top: 0;
    right: 0;
    width: 350px;
    text-align: right;
    margin-right: 20px;
}
/*.eTable-header .trShot{padding-right:20px;}*/
.eTable-header .trShot .dataShort{position:absolute;right:2px;top:0;width:20px;height:100%;}
.eTable-header .trShot .dataShort>span{width:100%;height:50%;display:block;cursor:pointer;background-position:0 0}
.eTable-header .trShot .dataShort>span.up{background:url(images/shortUp.png) no-repeat;}
.eTable-header .trShot .dataShort>span.down{background:url(images/shortDown.png) no-repeat;}
.eTable-header .trShot .dataShort>span:hover{background-position:-20px 0}



.eTable-container.fixedHeight .eTable-header th,.eTable-container td {
    height: 28px;
    line-height: 28px;
    text-align: center;
    border: 1px solid #e4e4e4;
    color: #868686;
}
.eTable-container th{
	background-color: #e5ecef;
}
.eTable-container tr.odd td{
	background-color: #f9f9f9;
}