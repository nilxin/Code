.eTable-container{
	width:100%;
	height:100%;
	overflow:hidden;
	position:relative;
    border: 1px solid #ebebeb;
    background:#fff;
	/*-moz-user-select: none; 
	-webkit-user-select:none; 
	-ms-user-select: none;*/
}

.borderTable .eTable-container{
    border-bottom:0;
}
.ePop-container .eTable-container{
    border-bottom:none;
}
.moving{
	-moz-user-select: none; 
	-webkit-user-select:none; 
	-ms-user-select: none
}
.borderTop{
	border-top:1px solid #ccc;
}
.borderBottom .eTable-footer{
	border-bottom:1px solid #ccc;
}
.eTable-container ul, .eTable-container li{
	width:100%;
	overflow:hidden;
	padding:0;
	margin:0;
	list-style:none;
}
.eTable-container tr.placeHolder{
	border-top:dashed 1px #ccc;
	border-bottom:dashed 1px #ccc;
	min-height:30px;
}
.eTable-container .v i{
    height:12px;
    width:16px;
    display:inline-block;
}
.eTable-container .v i.eTable-warning{
	background: url(images/ico_warning.png) left center no-repeat;
}
.eTable-container .right{
	text-align:right!important;
}
.eTable-container .left{
	text-align:left!important;
}
.eTable-container .center{
	text-align:center!important;
}
.eTable-container div.v .checkbox{
	display:inline-block;
	+display:block;
	height:16px;
	width:16px;
	background:url(images/checkbox2.png);
	text-indent:-1000px;
	cursor:pointer;
	margin:0;
}
.eTable-container .eTable-header div.v .checkbox{
	margin-top: 9px;
}
.eTable-container div.v span.checkboxDisabled{
	opacity:0.5;
	filter:alpha(opacity=50);
}
.eTable-container div.v span.checked{
    background: url(images/checkbox1.png);
}
.eTable-container div.v{
	/*padding:7px;*/
	text-align: left;
    text-indent: 10px;
    word-wrap: break-word;
    font-size: 12px;
}

/*eTable drag*/
.eTable-container .eTable-drag{
	width:5px;
	height:100%;
	position:absolute;
	cursor:col-resize;
	top:0;
	z-index:1;
}
.eTable-container .eTable-drag:hover, .eTable-container .eTable-drag-hover{
	background:url(images/line.gif) 2px center repeat-y;
}

/*eTable header*/
.eTable-header{
	/*background:#3755b3;*/
	border-bottom:1px solid #ebebeb;
}
.eTable-header div.v{
	color:#6f777c;
	height: 34px!important;
	line-height: 34px!important;
	padding-top: 0!important;
	padding-bottom: 0!important;
	font-weight:bold;
}
.eTable-header div.sort{
	cursor:pointer;
}
.eTable-header th{
	position:relative;
}
.eTable-header th.lastColumn{
	border-right:none;
}
.eTable-header th i.eTable-line{
	background-color:#ddd;
    display: none;
    height: 100%;
    position: absolute;
    right: 0;
    top: 0;
    width: 1px;
}

/*eTable data list*/
.eTable-list{
	overflow:auto;
	position:relative;
}
.borderTable .eTable-list{
	/*border:1px solid #dfdfdf;*/
}
.eTable-list >table{
	width:100%;
}
.eTable-list .no-data{
	text-align:center;
	line-height:200px;
}
.eTable-list td{
	height:47px;
    border-bottom:1px solid #ebebeb;
    border-right:1px solid #ebebeb;
}
.eTable-list td.lastColumn{
	border-right:0;
}
.eTable-list tr.odd td{
	background-color:#f7f7f7;
}
.eTable-list tr.even td{
	background-color:#fff;
}
.eTable-list tr:hover >td{
	background-color:#f5f5f5;
}
.eTable-list tr.selected >td{
	background-color:#d2e9fb;
}

/*eTable footer*/
.eTable-pager{
	border-top:1px solid #ebebeb;
	background:#fff;
	text-align:right;
	line-height:58px;
	position:relative;
	display:none;
    color:#6e808e;
}
.eTable-pager .total-info, .eTable-pager .data-size{
	padding:0 15px;
}
.eTable-pager span b{
	padding:0 3px;
}
.eTable-pager .ellipsis{
	cursor:default;
}
.eTable-pager select{
	margin:0 5px;
}
.eTable-pager .home-page,.eTable-pager .last-page,.eTable-pager .next-page,.eTable-pager .end-page{
	width: 30px;
	/*width: 50px;*/
	border: 1px solid #cecece;
}
.eTable-pager a {
    margin: -7px 4px;
    cursor: pointer;
    border: 1px solid #cccbcb;
    padding: 0 5px;
    height: 22px;
    border-radius: 5px;
    width: auto;
    line-height: 18px;
    display: inline-block;
    color: #6e808e;
    text-align: center;
}
.eTable-pager a.disabled {
  cursor: default;
  color: #888;
  background-color:#f4f8fb;
}
.eTable-pager span b .currentPage {
  font-size: 18px;
  color: #ea530a;
}
.eTable-pager a.current {
    background: #3c87c6;
    border: 1px solid #3c87c6;
    color: #fff;
    width: auto;
    height: 22px;
    display: inline-block;
    line-height: 18px;
    padding:0 5px;
}
/*.eTable-pager .data-size{
	position:absolute;
	left:0;
	top:0;
}*/
.eTable-pager .data-size select{
	border:1px solid #d1d1d1;
	border-radius:3px;
	height:25px;
	margin:6px 5px;
}
/*添加pager背景图片*/
.eTable-pager .home-page{
	background:url(images/arrow_1.png) 10px 6px no-repeat;
}
.eTable-pager .last-page{
	background:url(images/arrow_2.png) 12px 7px  no-repeat;
}
.eTable-pager .next-page{
	background:url(images/arrow_3.png) 13px 6px no-repeat;
}
.eTable-pager .end-page{
	background:url(images/arrow_4.png) 10px 6px no-repeat;
}
